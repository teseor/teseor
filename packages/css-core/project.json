{
  "name": "css-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/css-core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "commands": [
          "mkdir -p packages/css-core/dist",
          "cp -r packages/css-core/src/* packages/css-core/dist/"
        ],
        "parallel": false
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf packages/css-core/dist"
      }
    }
  },
  "tags": ["type:css", "scope:core"]
}
