{
  "name": "divider",
  "element": "div",
  "modifiers": {
    "vertical": {
      "type": "boolean"
    },
    "dashed": {
      "type": "boolean"
    },
    "position": {
      "values": ["start", "end"]
    }
  },
  "cssVars": [
    {
      "name": "--ui-divider-color",
      "default": "var(--ui-color-border)"
    },
    {
      "name": "--ui-divider-line-size",
      "default": "1px"
    },
    {
      "name": "--ui-divider-height",
      "default": "#{t.$unit}"
    },
    {
      "name": "--ui-divider-gap",
      "default": "calc(#{t.$unit} * 2"
    }
  ]
}
