    {
      "token": "--ui-pagination-item-bg-active",
      "default": "var(--ui-color-primary)",
      "description": "Active item background"
    },
    {
      "token": "--ui-pagination-item-color",
      "default": "var(--ui-color-text)",
      "description": "Item text color"
