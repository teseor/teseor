{
  "name": "card",
  "element": "div",
  "modifiers": {
    "variant": {
      "values": ["subtle", "muted"]
    },
    "size": {
      "values": ["sm", "lg"]
    },
    "flush": {
      "type": "boolean"
    }
  },
  "cssVars": [
    {
      "name": "--ui-card-padding",
      "default": "var(--ui-space-4)"
    },
    {
      "name": "--ui-card-border-width",
      "default": "var(--ui-border-width-sm)"
    },
    {
      "name": "--ui-card-bg",
      "default": "var(--ui-color-bg)"
    },
    {
      "name": "--ui-card-border-color",
      "default": "var(--ui-color-border)"
    },
    {
      "name": "--ui-card-radius",
      "default": "var(--ui-radius-md)"
    },
    {
      "name": "--ui-card-bg-subtle",
      "default": "var(--ui-color-bg-subtle)"
    },
    {
      "name": "--ui-card-bg-muted",
      "default": "var(--ui-color-bg-muted)"
    },
    {
      "name": "--ui-card-padding-sm",
      "default": "var(--ui-space-2)"
    },
    {
      "name": "--ui-card-padding-lg",
      "default": "var(--ui-space-6)"
    }
  ]
}
