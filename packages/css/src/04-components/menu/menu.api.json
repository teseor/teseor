      "default": "var(--ui-color-border)"
    },
    {
      "name": "--ui-menu-shadow",
      "default": "var(--shadow-lg)"
    },
    {
      "name": "--ui-menu-item-padding",
      "default": "calc(#{t.$unit} * 1"
    },
    {
      "name": "--ui-menu-item-radius",
      "default": "var(--ui-radius-sm)"
    },
    {
      "name": "--ui-menu-item-bg-hover",
      "default": "var(--ui-color-bg-subtle)"
