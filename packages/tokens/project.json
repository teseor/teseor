{
  "name": "tokens",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/tokens/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "commands": [
          "mkdir -p packages/tokens/dist",
          "cp -r packages/tokens/src/* packages/tokens/dist/"
        ],
        "parallel": false
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf packages/tokens/dist"
      }
    }
  },
  "tags": ["type:tokens", "scope:design-system"]
}
